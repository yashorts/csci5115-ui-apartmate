<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_apt_search">

    <fragment
        android:id="@+id/navigation_apt_search"
        android:name="com.example.group8project.ui.aptsearch.AptSearchFragment"
        android:label="Apartments"
        tools:layout="@layout/fragment_apt_search" />

    <fragment
        android:id="@+id/navigation_user_search"
        android:name="com.example.group8project.ui.usersearch.UserSearchFragment"
        android:label="Roommates"
        tools:layout="@layout/fragment_user_search" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.group8project.ui.chat.ChatFragment"
        android:label="Chat"
        tools:layout="@layout/fragment_chat" />
</navigation>